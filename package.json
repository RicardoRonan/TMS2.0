{
  "name": "metastack",
  "version": "1.0.0",
  "description": "MetaStack - Vue.js digital community platform for developers, designers, and creators",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "vite build --mode production",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint:css": "stylelint \"src/**/*.{css,scss,vue}\"",
    "fix:css": "stylelint \"src/**/*.{css,scss,vue}\" --fix",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
